{
  "name": "type-challenges-notes",
  "version": "0.0.0",
  "description": "My solutions to the questions of type-challenges.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Stan9726/type-challenges-notes.git"
  },
  "keywords": [
    "TypeScript",
    "type-challenges"
  ],
  "author": "Stan",
  "dependencies": {
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "lz-string": "^1.4.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.6",
    "@type-challenges/utils": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^17.0.25",
    "eslint": "^8.13.0",
    "typescript": "^4.6.3",
    "utility-types": "^3.10.0"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts",
    "rules": {
      "import/first": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-use-before-define": "off",
      "no-unused-expressions": "off",
      "prefer-const": "off",
      "no-alert": "off",
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  }
}
